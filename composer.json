{
  "name": "inpsyde/paypalplus-woocommerce",
  "description": "PayPal PLUS - the official WordPress Plugin for WooCommerce",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Inpsyde GmbH",
      "homepage": "https://inpsyde.com/",
      "email": "hallo@inpsyde.com",
      "role": "Company"
    },
    {
      "name": "Moritz Mei√üelbach",
      "email": "m.meisselbach@inpsyde.com",
      "role": "Developer"
    }
  ],
  "require": {
    "psr/log": "^1.0.2",
    "paypal/rest-api-sdk-php": "1.11.0"
  },
  "require-dev": {
    "brain/monkey": "^1.3",
    "inpsyde/monkery-test-case": "3.1.1",
    "coenjacobs/mozart": "^0.2.0",
    "inpsyde/php-coding-standards": "^0.13.3"
  },
  "suggest": {
    "coenjacobs/mozart": "We need a *GLOBAL* installation of mozart to namespace the PayPal SDK"
  },
  "autoload": {
    "psr-4": {
      "WCPayPalPlus\\": "src/",
      "Inpsyde\\Lib\\": "lib/"
    },
    "files": [
      "src/inc/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WCPayPalPlus\\Test\\": "tests/Helper",
      "Inpsyde\\Lib\\": "lib/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Inpsyde\\Lib\\",
      "dep_directory": "/lib/",
      "classmap_directory": "/lib/__classes/",
      "classmap_prefix": "InpsydeLib_",
      "packages": [
        "paypal/rest-api-sdk-php"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "./mozart.sh"
    ],
    "post-update-cmd": [
      "./mozart.sh"
    ]
  }
}
