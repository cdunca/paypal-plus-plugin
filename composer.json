{
  "name": "inpsyde/paypalplus-woocommerce",
  "description": "PayPal Plus - the official WordPress Plugin for WooCommerce",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Inpsyde GmbH",
      "homepage": "https://inpsyde.com/",
      "email": "hallo@inpsyde.com",
      "role": "Company"
    },
    {
      "name": "Moritz Mei√üelbach",
      "email": "m.meisselbach@inpsyde.com",
      "role": "Developer"
    }
  ],
  "require": {
    "psr/log": "^1.0.2"
  },
  "require-dev": {
    "brain/monkey": "^1.3",
    "mockery/mockery": "*",
    "inpsyde/monkery-test-case": "dev-master",
    "paypal/rest-api-sdk-php": "*",
    "coenjacobs/mozart": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "WCPayPalPlus\\": "src/",
      "Inpsyde\\Lib\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WCPayPalPlus\\Test\\": "tests/Helper",
      "Inpsyde\\Lib\\": "lib/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Inpsyde\\Lib\\",
      "dep_directory": "/lib/",
      "classmap_directory": "/lib/__classes/",
      "classmap_prefix": "InpsydeLib_",
      "packages": [
        "paypal/rest-api-sdk-php"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php -C \"vendor/bin/mozart\" compose"
    ],
    "post-update-cmd": [
      "php -C \"vendor/bin/mozart\" compose"
    ]
  }
}
